{
	"name": "fuzz-roadmap",
	"version": "2.5.0",
	"description": "A roadmapping tool that lets you forget about time estimate and focus on what matters",
	"author": "Adrien D. Ahlqvist",
	"bugs": "https://github.com/Armitage35/fuzzy-roadmap/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "github.com:Armitage35/fuzzy-roadmap.git"
	},
	"scripts": {
		"serve": "vue-cli-service serve",
		"start": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"sass-lint": "sass-lint -c ./sass-lint.yml '**/*.scss' -v -q",
		"deploy": "bash src/deploy.sh"
	},
	"dependencies": {
		"core-js": "^3.3.2",
		"html2canvas": "^1.0.0-rc.5",
		"izitoast": "^1.4.0",
		"tippy.js": "^5.0.3",
		"vue": "^2.6.10",
		"vue-router": "^3.1.3",
		"vuedraggable": "^2.23.2"
	},
	"devDependencies": {
		"@vue/cli": "^4.0.2",
		"@vue/cli-plugin-babel": "^3.12.0",
		"@vue/cli-plugin-eslint": "^4.0.3",
		"@vue/cli-service": "^3.12.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"node-sass": "^4.12.0",
		"sass-lint": "^1.13.1",
		"sass-loader": "^8.0.0",
		"serve": "^11.2.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
